{
  "name": "jiejia/a-ripple-song",
  "version": "0.7.0",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "All-in-one podcast theme for WordPress",
  "homepage": "https://doc-podcast.aripplesong.me/",
  "authors": [
    {
      "name": "Jiejia",
      "homepage": "https://github.com/jiejia/"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/jiejia/a-ripple-song/issues"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "cmb2/cmb2": "^2.11",
    "htmlburger/carbon-fields": "^3.6",
    "james-heinrich/getid3": "^1.9",
    "roots/acorn": "^5.0"
  },
  "require-dev": {
    "laravel/pint": "^1.20"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}